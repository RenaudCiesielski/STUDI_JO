@model List<Category>

<h1>Catégories Tickets</h1>
<table class="table table-bordered table-striped">
    <thead>
    <tr>
        <th>
            Catégorie ticket
        </th>
        <th>
            Description
        </th>
        <th>
            Prix
        </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td>@obj.Name</td>
                <td>@obj.Description</td>
                <td>@obj.Price</td>
            </tr>
        }
    </tbody>
</table>